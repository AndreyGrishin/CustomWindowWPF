<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CustomWindow.Themes">
    

    <!--#region Colors-->

    <SolidColorBrush x:Key="FirstMainColor" Color="#FCFCFC" />
    <SolidColorBrush x:Key="SecondMainColor" Color="#FFF9F9F9" />
    <SolidColorBrush x:Key="ThirdMainColor" Color="#C6C6C6" />
    <SolidColorBrush x:Key="FourthMainColor" Color="#B9C616" />


    <SolidColorBrush x:Key="TextColor" Color="#101010" />

    <SolidColorBrush x:Key="WindowButtonsForeground" Color="#222222" />
    <SolidColorBrush x:Key="WindowButtons.Foreground.MouseOver" Color="White" />


    <SolidColorBrush x:Key="Button.Static.Border" Color="Black"/>
    <SolidColorBrush x:Key="Button.Static.Background" Color="Transparent"/>
    <SolidColorBrush x:Key="Button.Pressed.Background" Color="#FF8F8F8F"/>
    <SolidColorBrush x:Key="Button.Pressed.Ellipse" Color="White"/>
    <!--#endregion-->
    

</ResourceDictionary>