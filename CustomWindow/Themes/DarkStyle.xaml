<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:CustomWindow.Themes">


    <!--#region Colors-->

    <SolidColorBrush x:Key="FirstMainColor" Color="#FF1F1F1F" />
    <SolidColorBrush x:Key="SecondMainColor" Color="#FF2F2F2F" />
    <SolidColorBrush x:Key="ThirdMainColor" Color="#FF2B2B2B" />
    <SolidColorBrush x:Key="FourthMainColor" Color="#393939" />

    <SolidColorBrush x:Key="TextColor" Color="White" />

    <SolidColorBrush x:Key="WindowButtonsForeground" Color="White" />
    <SolidColorBrush x:Key="WindowButtons.Foreground.MouseOver" Color="White" />

    <SolidColorBrush x:Key="Button.Static.Border" Color="White"/>
    <SolidColorBrush x:Key="Button.Static.Background" Color="Transparent"/>
    <SolidColorBrush x:Key="Button.Pressed.Background" Color="#FF8F8F8F"/>
    <SolidColorBrush x:Key="Button.Pressed.Ellipse" Color="White"/>

    <!--#endregion-->


</ResourceDictionary>